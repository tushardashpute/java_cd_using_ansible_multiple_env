[Unit]
Description=A Spring Boot application
After=syslog.target

[Service]
Type=forking

Environment=JAVA_HOME=/usr/lib/jvm/java-1.8.0-openjdk-1.8.0.282.b08-1.amzn2.0.1.x86_64/jre
ExecStart=/opt/gs-spring-boot
